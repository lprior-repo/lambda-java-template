<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppress false positives for AWS Lambda Core library -->
    <suppress>
        <notes><![CDATA[
        AWS Lambda runtime is managed by AWS and these dependencies are only present in the runtime environment
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.amazonaws/aws\-lambda\-java\-.*@.*$</packageUrl>
        <cve>CVE-2020-8908</cve>
    </suppress>

    <!-- Suppress false positives for Jackson -->
    <suppress>
        <notes><![CDATA[
        Jackson CVEs that don't apply to our usage pattern
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/jackson\-.*@.*$</packageUrl>
        <cve>CVE-2020-36518</cve>
    </suppress>
</suppressions>