<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!--
        OWASP Dependency Check Suppression File

        This file contains suppression rules for false positives and known issues
        that are not applicable to our Lambda runtime environment.
    -->

    <!-- Suppress false positives for AWS Lambda Java runtime dependencies -->
    <suppress>
        <notes><![CDATA[
            AWS Lambda runtime environment is managed by AWS and receives security updates automatically.
            The Lambda Java runtime uses Amazon Corretto which includes security patches.
        ]]></notes>
        <gav regex="true">^com\.amazonaws:aws-lambda-java-.*</gav>
        <cpe regex="true">.*</cpe>
    </suppress>

    <!-- Suppress Jackson false positives for versions used in AWS Lambda runtime -->
    <suppress>
        <notes><![CDATA[
            Jackson vulnerabilities that are not applicable to server-side Lambda functions
            that don't deserialize untrusted user input directly.
        ]]></notes>
        <gav regex="true">^com\.fasterxml\.jackson\.core:.*</gav>
        <vulnerabilityName regex="true">.*XXE.*</vulnerabilityName>
    </suppress>

    <!-- Suppress test-only dependencies -->
    <suppress>
        <notes><![CDATA[
            JUnit is only used in test scope and not included in deployed artifacts.
        ]]></notes>
        <gav regex="true">^org\.junit\..*</gav>
        <cpe regex="true">.*</cpe>
    </suppress>
</suppressions>